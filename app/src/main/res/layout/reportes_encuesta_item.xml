                    android:text="TextView" />

                <TextView
                    android:id="@+id/textView140"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#283593"
                    android:text="Tiempo de entrega"
                    android:textColor="#FFFFFF" />

                <TextView
                    android:id="@+id/r5cal5_REI"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView" />

                <TextView
                    android:id="@+id/textView145"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#283593"
                    android:text="Calidad / Precio"
                    android:textColor="#FFFFFF" />

                <TextView
                    android:id="@+id/r5cal6_REI"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView" />

                <TextView
                    android:id="@+id/textView146"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#283593"
                    android:text="Recomendaciones"
                    android:textColor="#FFFFFF" />

                <TextView
                    android:id="@+id/recomendacion_REI"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView" />

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>